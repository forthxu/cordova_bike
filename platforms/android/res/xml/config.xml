<?xml version='1.0' encoding='utf-8'?>
<widget id="cc.dreamlist.bike" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="BaiduLocation">
        <param name="android-package" value="com.qdc.plugins.baidu.BaiduLocation" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="ThemeableBrowser">
        <param name="android-package" value="com.initialxy.cordova.themeablebrowser.ThemeableBrowser" />
    </feature>
    <allow-intent href="market:*" />
    <name>厦门自行车</name>
    <description>
        厦门公共自行车站点信息查询
    </description>
    <author email="forthxu@gmail.com" href="http://dreamlist.cc">
        dreamlist.cc
    </author>
    <content src="index.html" />
    <icon src="res/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
